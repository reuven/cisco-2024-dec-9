    "menu = {'sandwich':10, 'tea':8, 'apple':2, 'cake':5}\n",
    "for key, value in menu.items():\n",
    "    print(f'{key} = \\t{value}')\n",
    "f = 'bobsfile'\n",
    "with open(f, 'w') as f:\n",
    "    for key, value in menu.items():\n",
    "        f.write(f'{key}={value}\\n')\n"
